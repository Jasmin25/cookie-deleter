{
  "manifest_version": 3,
  "name": "Cookie Deleter",
  "version": "1.0",
  "description": "Deletes cookies for specified sites",
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
